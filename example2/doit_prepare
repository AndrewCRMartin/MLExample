CSV2ARFF=csv2arff
DATA=humVar_withNoErrorsNoMissing_PD50pctSNP50pct.key.header.csv

$CSV2ARFF -v -norm -limit=500 inputs.txt dataset $DATA >HumVar_500_1.arff
$CSV2ARFF -v -norm -limit=500 inputs.txt dataset $DATA >HumVar_500_2.arff
$CSV2ARFF -v -norm -limit=500 inputs.txt dataset $DATA >HumVar_500_3.arff
$CSV2ARFF -v -norm -limit=500 inputs.txt dataset $DATA >HumVar_500_4.arff
$CSV2ARFF -v -norm -limit=500 inputs.txt dataset $DATA >HumVar_500_5.arff
